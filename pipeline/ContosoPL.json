{
	"name": "ContosoPL",
	"properties": {
		"activities": [
			{
				"name": "RetreiveAllTables_foreach",
				"type": "ForEach",
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.tableList",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "CopyContosoTables",
							"type": "Copy",
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"typeProperties": {
								"source": {
									"type": "SqlServerSource",
									"sqlReaderQuery": "SELECT * FROM [@{item().TABLE_SCHEMA}].[@{item().TABLE_NAME}]"
								},
								"sink": {
									"type": "ParquetSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSetting"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "ContosoDataset",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "ContosoSink",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"tableList": {
				"type": "array"
			}
		}
	}
}